<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="400" d:DesignHeight="100"
			 xmlns:vm="clr-namespace:SummerSchoolGUI.ViewModels.Primitives"
             x:Class="SummerSchoolGUI.Views.Primitives.Vector2View"
			 x:DataType="vm:Vector2ViewModel">
	<Design.DataContext>
		<vm:Vector2ViewModel />
	</Design.DataContext>
	<Grid RowDefinitions="*" ColumnDefinitions="auto, *, *" Margin="5">
		<Label Content="{Binding PropertyName}"
			   Grid.Row="0" Grid.Column="0"
			   VerticalAlignment="Center"/>
		<ContentControl Content="{Binding X}" Grid.Column="1"/>
		<ContentControl Content="{Binding Y}" Grid.Column="2"/>
	</Grid>
</UserControl>
